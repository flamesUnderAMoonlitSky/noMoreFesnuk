{
  "manifest_version": 2,
  "name": "No more fesnuk!",
  "version": "1.1.1",
  "description": "Redirects facebook.com to a random coding problem with difficulty and website preferences.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.facebook.com/*",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Set Preferences",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },

  "web_accessible_resources": ["ding.mp3"],

  "applications": {
    "gecko": {
      "id": "{c1a115b2-6815-4703-920f-5747e0bd6f73}"
    }
  }
}
